<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.stu.com.app3">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".actitvty.MyBehaviorActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".actitvty.BottomSheetActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".TouchHelper.LineActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TouchHelper.LineActivity2"
            android:label="滑动显示隐藏布局"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TouchHelper.GridActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TouchHelper.StaggeredActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>